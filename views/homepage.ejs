    <title>Document</title>
    <link rel="stylesheet" href="style.css">
    <style>
        #map {
            height: 400px;
            /* The height is 400 pixels */
            width: 100%;
            /* The width is the width of the web page */
        }
    </style>
</head>

<body>
    <main>
        <header>

        </header>
        <div class="content">
            <div class="image-gallery">
                <% result.images.forEach(img=> { %>
                    <img src=<%=img %> alt="home img">

                    <% }); %>
            </div>
            <address>
                <%= result.address %>
            </address>

        </div>
        <footer>
            <%= result.price %>
        </footer>
    </main>
